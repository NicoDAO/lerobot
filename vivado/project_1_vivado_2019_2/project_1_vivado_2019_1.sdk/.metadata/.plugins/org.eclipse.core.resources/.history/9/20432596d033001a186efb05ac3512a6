#include "GereMoteur.h"

#include "MonPWM.h"

GereMoteur::GereMoteur() {

}

GereMoteur::~GereMoteur() {

}

void GereMoteur::handler() {
	lpwm.RegleRapportCyclique(1000);//test
	lapause(0);
}

void GereMoteur::SetAdresseMoteur(u32 add) {
	lpwm.setBaseAddr(add); // on regle l'adresse du PWM
}
